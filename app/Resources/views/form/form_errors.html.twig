{% block form_errors %}

        {% if errors|length > 0 %}
        <div class="alert alert-danger alert-dismissible alert-block" role="alert" style="margin-top: 10px">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>

            {% for error in errors %}
                {{ error.message }}
            {% endfor %}
        </div>
        {% endif %}

{% endblock form_errors %}